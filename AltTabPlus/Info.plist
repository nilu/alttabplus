<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>AltTabPlus</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0.0</string>
	<key>CFBundleVersion</key>
	<integer>1</integer>
	<key>LSMinimumSystemVersion</key>
	<string>$(MACOSX_DEPLOYMENT_TARGET)</string>
	<key>LSUIElement</key>
	<true/>
	<key>NSHighResolutionCapable</key>
	<true/>
	<key>NSPrincipalClass</key>
	<string>NSApplication</string>
	<key>NSAppleEventsUsageDescription</key>
	<string>AltTabPlus needs to control Finder to open new windows when needed.</string>
	<key>NSSystemEventsUsageDescription</key>
	<string>AltTabPlus needs to access System Events to manage Finder windows.</string>
	<key>NSHumanReadableCopyright</key>
	<string>Copyright Â© 2025 Pandey Labs Inc. All rights reserved.</string>
	<key>CFBundleGetInfoString</key>
	<string>AltTabPlus by Nilesh Pandey</string>
	<key>SMPrivilegedExecutables</key>
	<dict>
		<key>com.yourdomain.AltTabPlus</key>
		<string>identifier "com.yourdomain.AltTabPlus" and anchor apple generic</string>
	</dict>
	<key>CFBundleIconFile</key>
	<string>AppIcon</string>
</dict>
</plist>
